{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "." ,
    "paths": {
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@helpers/*": [
        "src/helpers/*"
      ],
       "@services/*": [
         "src/services/*"
       ],
       "@store/*": [
         "src/store/*"
       ],
       "@ourtypes/*": [
        "src/types/*"
      ],
      "@tests/*": [
        "__tests__/*"
      ],
    },
 }
}
